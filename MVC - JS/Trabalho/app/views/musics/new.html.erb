<h3>Create a Music</h3>
<%= form_for @music do |f| %>
    <p>Enter the song title: <%= f.text_field :name, placeholder: "music name" %></p>
    <p>Explicit:          <%= f.check_box :explicit %></p>
    <p>Album_id:          <%= f.text_field :album_id, placeholder: "album id" %></p>
    <p>Genre:             <%= f.text_field :genre, placeholder: "genre name" %></p>

    <%= f.submit %>
<% end %>